<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="90dp"
    android:background="#FDFDFD">

    <TextView
        android:id="@+id/itemTIpo"
        android:layout_width="132dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        android:text="TextView"
        android:textAllCaps="true"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toEndOf="@+id/itemData"
        app:layout_constraintTop_toBottomOf="@+id/itemEstadio" />

    <TextView
        android:id="@+id/itemResultado"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/itemEstadio"
        app:layout_constraintTop_toBottomOf="@+id/itemCidade" />

    <TextView
        android:id="@+id/itemData"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/itemEstadio" />

    <TextView
        android:id="@+id/itemCidade"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/itemEstadio"
        app:layout_constraintTop_toBottomOf="@+id/itemEstado" />

    <TextView
        android:id="@+id/itemSelecoes"
        android:layout_width="160dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:autoText="true"
        android:background="#FFFFFF"
        android:elegantTextHeight="false"
        android:freezesText="false"
        android:text="TextView"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/itemPlacar"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:text="TextView"
        android:textAlignment="viewEnd"
        app:layout_constraintStart_toEndOf="@+id/itemSelecoes"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/itemEstadio"
        android:layout_width="210dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:autoText="true"
        android:background="#FFFFFF"
        android:elegantTextHeight="true"
        android:text="TextView"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/itemSelecoes" />

    <TextView
        android:id="@+id/itemEstado"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toEndOf="@+id/itemEstadio"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>